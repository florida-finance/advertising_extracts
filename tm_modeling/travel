
-- @WbResult tm_modeling
SELECT *
FROM financial_reporting.tm_modeling
where 
(lower(sold_to_customer_name) LIKE '%tour%' and sold_to_customer_name != 'Touro College & University System (SSC)')
OR
(lower(sold_to_customer_name) LIKE '%travel%' and sold_to_customer_name != 'World Travelers Antique Shoppe')
OR 
order_sales_subcategory = 'Travel Agencies'
OR
order_sales_subcategory = 'Hotels, Resorts & Accomodations'
OR
lower(sold_to_customer_name) LIKE '%visit%'
OR
lower(sold_to_customer_name) LIKE '%cruise%'
OR
lower(sold_to_customer_name) LIKE '%discover%'
OR
lower(sold_to_customer_name) LIKE '%vacation%'

